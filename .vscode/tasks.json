{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Create Python Notebook",
      "type": "shell",
      "command": "cp Python_Analysis/Templates/nb_template.ipynb Python_Analysis/Notebooks/${input:param1}.ipynb && sed -i 's/Notebooktitle/${input:param2}/' Python_Analysis/Notebooks/${input:param1}.ipynb && code Python_Analysis/Notebooks/${input:param1}.ipynb"
    }
  ],
  "inputs": [
    {
      "id": "param1",
      "description": "Author's initials, month number, title ",
      "default": "ag_0_title",
      "type": "promptString"
    },
    {
      "id": "param2",
      "description": "Notebook Title",
      "default": "Sample Notebook Title",
      "type": "promptString"
    }
  ]
}
